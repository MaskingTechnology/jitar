{
  "name": "jitar-runtime",
  "version": "0.3.8",
  "description": "JavaScript runtime library for the Jitar runtime.",
  "author": "Masking Technology <info@masking.tech> (https://jitar.dev)",
  "license": "MIT",
  "type": "module",
  "private": true,
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "test": "vitest run",
    "test-coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf dist && rm -rf build",
    "build:tsc": "tsc -p tsconfig.json",
    "build:rollup": "rollup -c rollup.config.js",
    "build": "npm run clean && npm run build:tsc && npm run build:rollup"
  },
  "dependencies": {
    "jitar-serialization": "^0.3.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MaskingTechnology/jitar"
  },
  "bugs": {
    "url": "https://github.com/MaskingTechnology/jitar/issues"
  },
  "homepage": "https://jitar.dev",
  "keywords": [
    "javascript",
    "runtime",
    "jitar"
  ]
}
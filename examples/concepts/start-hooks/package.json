{
    "name": "jitar-start-hooks-example",
    "type": "module",
    "private": true,
    "scripts": {
        "clean": "rimraf dist",
        "compile": "tsc",
        "copy": "cpx -u 'src/index.html' dist",
        "build": "npm-run-all clean copy compile",
        "standalone": "node --experimental-network-imports dist/server.js --config=services/standalone.json",
        "client": "node --experimental-network-imports dist/nodeClient.js"
    },
    "dependencies": {
        "jitar": "^0.4.0"
    },
    "devDependencies": {
        "cpx2": "^4.2.0",
        "npm-run-all": "^4.1.5",
        "rimraf": "^4.1.2"
    }
}
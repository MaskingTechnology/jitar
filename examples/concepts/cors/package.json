{
    "name": "jitar-cors-example",
    "type": "module",
    "private": true,
    "scripts": {
        "clean": "rimraf .jitar",
        "compile": "tsc && jitar build",
        "copy": "cpx -u 'src/index.html' dist",
        "build": "npm-run-all clean copy compile",
        "standalone": "jitar start --service=services/standalone.json",
        "client": "node dist/client.js"
    },
    "dependencies": {
        "jitar": "*",
        "express": "*"
    },
    "devDependencies": {
        "cpx2": "*",
        "npm-run-all": "*",
        "rimraf": "*"
    }
}
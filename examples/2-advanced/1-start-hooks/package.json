{
    "name": "jitar-start-hooks-example",
    "type": "module",
    "scripts": {
        "clean": "rimraf dist",
        "compile": "tsc",
        "copy": "cpx -u 'src/index.html' dist",
        "build": "npm-run-all clean copy compile",
        "start": "node --experimental-network-imports --experimental-fetch dist/server.js --config=jitar.json"
    },
    "dependencies": {
        "jitar": "^0.1.0"
    },
    "devDependencies": {
        "cpx": "^1.5.0",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2"
    }
}